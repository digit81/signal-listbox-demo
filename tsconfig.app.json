{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@app/*": ["src/app/*"],
      "@data-layer": ["libs/data-layer/src/index.ts"],
      "@state": ["libs/state/src/index.ts"],
      "@shared/models": ["libs/shared/models/src/lib/models/index.ts"],
      "@ui/toaster": ["libs/ui/toaster/src/index.ts"],
      "@ui/listbox": ["libs/ui/listbox/src/index.ts"],
      "@ui/card": ["libs/ui/card/src/index.ts"],
      "@ui/vegetable-form": ["libs/ui/vegetable-form/src/index.ts"]
    }
  },
  "files": ["src/main.ts"],
  "include": ["src/**/*.d.ts"],
  "exclude": [
    "cypress/**/*",
    "cypress.config.ts",
    "**/*.cy.ts",
    "**/*.cy.js",
    "**/*.cy.tsx",
    "**/*.cy.jsx"
  ]
}
